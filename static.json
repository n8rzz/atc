{
  "root": "assets/",
  "https_only": true,
  "routes": {
    "/aircraft/*": "/aircraft/",
    "/airlines/*": "/airlines/",
    "/airports/*": "/airports/",
    "/fonts/*": "/fonts/",
    "/images/*": "/images/",
    "/scripts/*": "/scripts/",
    "/style/*": "/style/",
    "/**": "index.html"
  },
  "headers": {
    "/": {
        "Cache-Control": "no-store, no-cache"
    },
    "**.js": {
        "Cache-Control": "public, max-age=31536000"
    },
    "**.css": {
        "Cache-Control": "public, max-age=31536000"
    },
    "**.jpg": {
        "Cache-Control": "public, max-age=31536000"
    },
    "**.png": {
        "Cache-Control": "public, max-age=31536000"
    },
    "**.gif": {
        "Cache-Control": "public, max-age=31536000"
    },
    "**.json": {
        "Cache-Control": "public, max-age=31536000"
    }
  }
}
